{% extends 'base.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
    <h1>Contact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact.Email }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ contact.Phone }}</td>
            </tr>
            <tr>
                <th>Massage</th>
                <td>{{ contact.Massage }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contact.Name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contact_index') }}">back to list</a>

    <a href="{{ path('app_contact_edit', {'id': contact.id}) }}">edit</a>

    {{ include('contact/_delete_form.html.twig') }}
{% endblock %}
