    function tinh_tong_tien(className){
        var price = parseInt($(".js-this-product").attr("data-price"));
        var unprice = parseInt($(".js-this-product").attr("data-unprice"));;
  		if(unprice > 0){
  			unprice = unprice;
        }else {
  			unprice = price;
  		}
        $(className).each(function(index,checkbox){
            if(checkbox.checked) {
  				if(parseInt($(this).attr('data-unprice')) == 0){
  					price += parseInt($(this).attr('data-price'));
  					unprice += parseInt($(this).attr('data-price'));
  				}else{
                    price += parseInt($(this).attr('data-price'));
                    unprice += parseInt($(this).attr('data-unprice'));
  				}
            }
        });
        $('.js-pass-price').html(formatCurrency(price) + 'đ');
  
  		if(unprice > 0){
  			$('.js-pass-unprice').html(formatCurrency(unprice) + 'đ').show();
  		}
  		else $('.js-pass-unprice').hide();
  		
  		$("#count-pro-selected").html($(".js-check-select:checked").length);
    }
  
    function check_select(){
        $('.js-check-select').on('click', function(e){
            if($(e.target).parent('.pro-item').hasClass('w-select') == true){

            }else{
                if($(e.target).parent('.pro-item').hasClass('select') == true){
                    $(e.target).parent('.pro-item').removeClass('select');
                }else{
                    $(e.target).parent('.pro-item').addClass('select');
                }
            }
            tinh_tong_tien('.js-price');
  
  			$("#count-pro-selected").html($(".js-check-select:checked").length);
        });
    }
    check_select();
