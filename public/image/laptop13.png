function kiemtratonkho_2022(sku, holder) {

    var url = "/ajax/laptop88.php?action=get-stock-status&sku=" + sku;
    var html = "";
    var kho = "";

    $.getJSON(url, function (data) {
        //console.log(data)
        if (data.length > 0) {
            $.each(data, function (index, value) {
                if (data.length > 0 && value.quantity > 0) {
                    check_list_store.forEach(function (store_data) {
                        console.log('tesss', value.store_name)
                        if (value.store_name == store_data.store_name) {
                            kho += `
                                <div class="item js-item" data-erp="`+ store_data.store_name + `">
                                    <a href="https://zalo.me/`+ store_data.phone + `" target="_blank" class="item-map"> 
                                       <i class="fa fa-phone"></i> `+ store_data.phone + `
                                    </a>
                                    <a href="`+ store_data.map + `" target="_blank" title="` + store_data.address + `" class="item-add"> 
                                       <i class="fas fa-map-marker-alt"></i> `+ store_data.address + `
                                    </a>
                                </div>
                            `;
                        }
                    });

                } else {
                    $('#content-store').css('display','none');
  					$('#khongcokhohang').css('display','block');
                }
            });
            html += kho;
            if (html == '') {
                $('#content-store').css('display','none');
                $('#khongcokhohang').css('display','block');
            } else {
                 $(holder).html(html);
                  $('#khongcokhohang').css('display','none');
                  $('#content-store').css('display','block');
                $('.product_store').show();
            }
        } else {
            $('.product_store').hide();
        }
    });
}
  
$(document).ajaxStop(function(){  
   setTimeout(function(){ 
      $('#total-store').html($('#js-store-list .js-item').length); // tong kho thoa man
  
   }, 500);
})  
  
  
function lay_danh_sach_cua_hang_theo_tinh(province_id){
 if(province_id == 0){ 
   kiemtratonkho_2022($("#js_data_sku").val(), '#js-store-list');
 }else{  
    var html = "";
    var kho = [];
  
  $('#js-store-list .item').each(function(){
  
     erp_id.push($(this).attr('data-erp')) 
  
  });
  
    check_list_province.forEach(function(item){
       //console.log(item);
       if(item.id == province_id){
  		 var list_store_new = item.store_list;
          list_store_new.forEach(function(key){
              erp_id.filter(function(item){
                if (item === key.store_name){  
                  kho += `
                    <div class="item js-item" data-erp="`+ key.name +`">
                      <a href="https://zalo.me/`+ key.phone +`" target="_blank" class="item-map"> 
                        <i class="fa fa-phone"></i> `+ key.phone +`
                      </a>
                      <a href="`+ key.map +`" target="_blank" title="`+ key.address +`" class="item-add"> 
                        <i class="fas fa-map-marker-alt"></i> `+ key.address +`
                      </a>
                    </div>
                  `;
   			   }
             });
  		  })
  	  }
   })
    html += kho;
    $('#js-store-list').html(html);
  
   setTimeout(function(){ 
      $('#total-store').html($('#js-store-list .js-item').length); // tong kho thoa man
  
   }, 500);
  }
}
</script>
  
  

    


